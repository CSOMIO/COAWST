PROJECT 'Inlet Test' ' '
 'INLET test'
 'Bathymetry: flat bottom'
 'COMMENTS'

MODE NONSTATIONARY TWODIMENSIONAL

SET DEPMIN 0.10 INRHOG 1 NAUTICAL
COORDINATES CARTESIAN

&& KEYWORDS TO CREATE AND READ COMPUTATIONAL GRID &&
CGRID CURVILINEAR 156 86 EXC 9.999000e+003 &
        CIRCLE 36 0.04 1.0 20
READGRID COORDINATES 1 'Projects/Inlet_test/Swanonly/inlet_test_grid_coord_ref5.grd' 4 0 0 FREE

&& KEYWORDS TO CREATE AND READ BATHYMETRY GRID &&
INPGRID BOTTOM CURVILINEAR 0 0 156 86 EXC 9.999000e+003        
READINP BOTTOM  1 'Projects/Inlet_test/Swanonly/inlet_test_bathy_ref5.bot' 4 0 FREE
 
& Restart name **********************************
&INIT HOTSTART 'swan_restart.dat'

& PHYSICS  **************************************
GEN3
FRICTION MADSEN 0.05
OFF QUAD

BLOCK 'COMPGRID' NOHEADER 'depth_ref5.mat'  LAY 4 DEPTH 1.    OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'watlev_ref5.mat' LAY 4 WATLEV 1.   OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'botlev_ref5.mat' LAY 4 BOTLEV 1.   OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'vel_ref5.mat'    LAY 4 VEL 1.      OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'fric_ref5.mat'   LAY 4 FRCOEF  1.  OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'dissip_ref5.mat' LAY 4 DISSIP 1.   OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'hsig_ref5.mat'   LAY 4 HSIGN 1.    OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'rtp_ref5.mat'    LAY 4 RTP 1.      OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'tmbot_ref5.mat'  LAY 4 TMBOT 1.    OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'ubot_ref5.mat'   LAY 4 UBOT 1.     OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'wdir_ref5.mat'   LAY 4 DIR 1.      OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'wlen_ref5.mat'   LAY 4 WLEN 1.     OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'qb_ref5.mat'     LAY 4 QB 1.       OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'xp_ref5.mat'     LAY 4 XP 1.       OUTPUT 20000101.000000 1 HR
BLOCK 'COMPGRID' NOHEADER 'yp_ref5.mat'     LAY 4 YP 1.       OUTPUT 20000101.000000 1 HR

PROP BSBT

COMPUTE NONSTATIONARY 20000101.000000 30 SEC 20000101.120000
HOTFILE 'swangrid2.rst'

STOP
