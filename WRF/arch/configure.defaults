OMP             =       -qsmp=noauto:noopt
OMPCPP          =       -D_OPENMP
FC              =       xlf90_r
SFC		=	$(FC)
LD              =       xlf90_r
CC              =       cc_r
SCC		=	$(CC)
RWORDSIZE       =       $(NATIVE_RWORDSIZE)
